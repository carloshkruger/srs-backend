{
  "name": "srs-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/infra/http/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "jest --runInBand",
    "test:cov": "jest --no-cache --coverage --runInBand",
    "test:e2e": "npx dotenv -e .env.test -- npx prisma migrate dev && jest -i --no-cache --runInBand --config ./tests-e2e/jest-e2e.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carloshkruger/srs-backend.git"
  },
